<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">

	<class name="pe.joedayz.ejemplos.cinedayz.dominio.Busqueda" table="busqueda">
		<id name="codigoBusqueda" column="buscodig" length="5">
			<generator class="increment" />
		</id>
		<property name="cine" column="buscineb" length="20" />
		<property name="artista" column="busartis"  length="40"/>
		<property name="horarios" column="bushorar"  />
		<property name="tipoPelicula" column="bustipop"  />
		<property name="subtitulos" column="bussubti"  />
		<property name="pais" column="buspaiso"  />
		<property name="publicoObjetivo" column="buspubob"  />
		<property name="tipoEmision" column="bustipoe"  />
				
				
	</class>
</hibernate-mapping>

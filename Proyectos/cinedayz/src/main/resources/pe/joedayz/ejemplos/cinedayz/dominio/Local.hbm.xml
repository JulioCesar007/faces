<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">

	<class name="pe.joedayz.ejemplos.cinedayz.dominio.Local" table="local">
		<id name="localCodigo" column="loccodig" length="5">
			<generator class="assigned" />
		</id>
		<property name="localNombre" column="locnombr" length="20" />
		<property name="localDireccion" column="locdirec"  length="40"/>
		<property name="localManager" column="usucodig"  />
				
	</class>
</hibernate-mapping>


<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">

	<class name="pe.joedayz.ejemplos.cinedayz.dominio.Permiso" table="permiso">
		<id name="codigo" column="percodig" length="5">
			<generator class="assigned" />
		</id>
		
		<property name="acceso" column="acccodig" length="5" />
		<property name="usuario" column="usucodig" length="20" />
		<property name="fechaBaja" column="perfecba"  />
		<property name="fechaInicio" column="perfecin"  />
		<property name="status" column="perstatu" length="1" />
		
	</class>
</hibernate-mapping>
